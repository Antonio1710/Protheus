User Function ADVEN088P() // U_ADVEN088P()

	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Declaracao de Variaveis                                             ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	Local aArea := GetArea()
	Local cPerg	:= "SFLIBFIN"
	Local cDtDe	:= ""
	Local cDtAt := ""
	Local cPlaca:= ""
	Local cPedDe:= ""
	Local cPedAt:= ""

	U_ADINF009P(SUBSTRING(ALLTRIM(PROCNAME()),3,LEN(ALLTRIM(PROCNAME()))) + '.PRW',SUBSTRING(ALLTRIM(PROCNAME()),3,LEN(ALLTRIM(PROCNAME()))),'Fun็ใo para envio de status dos pedidos de venda ao SalesForce pelo departamento financeiro.')
	
	//
	//criaPerg(cPerg)

	//
	If ! Pergunte(cPerg,.T.)
		RestArea(aArea)
		Return Nil
		
	EndIf
	
	//
	cDtDe := DToS(mv_par01)
	cDtAt := DToS(mv_par02)
	cPlaca:= Alltrim(cValToChar(mv_par03))
	cPedDe:= Alltrim(cValToChar(mv_par04))
	cPedAt:= Alltrim(cValToChar(mv_par05))
	
	MsAguarde({|| procAprv(cDtDe,cDtAt,cPlaca,cPedDe,cPedAt) },"Aguarde","Processando pedidos aprovados...")
	
	MsAguarde({|| procBlq(cDtDe,cDtAt,cPedDe,cPedAt) },"Aguarde","Processando pedidos reprovados...")
	
	//
	RestArea(aArea)
	
Return Nil
/*

