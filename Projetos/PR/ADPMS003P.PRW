User Function zzM1Com()

Local cModelPad := FWModelActive()
Local cCodigo   := oModelPad:GetValue("FORMZCH", "ZCH_CODIGO")
Local cDescri   := oModelPad:GetValue("FORMZCH", "ZCH_NOME")
Local nOpc      := oModelPad:GetOperation()
Local lRet      := .t.

U_ADINF009P('ADPMS003P' + '.PRW',SUBSTRING(ALLTRIM(PROCNAME()),3,LEN(ALLTRIM(PROCNAME()))),'CADASTRO SPONSOR - Modelo 1 com validacoes')

// Se for inclusao
If nOpc == MODEL_OPERATION_INSERT
	
	RecLock("ZCH", .T.)
		ZCH_FILIAL := FWxFilial("ZCH")
		ZCH_CODIGO := cCodigo
		ZCH_NOME   := cDescri
	ZCH->( msUnLock() )
	ConfirmSX8()
	
	Aviso("Aten็ใo", "Inclusใo realizada!", {"OK"}, 03)
		
// Se for alteracao
ElseIf nOpc == MODEL_OPERATION_UPDATE
	
	RecLock("ZCH", .F.)
		ZCH_NOME := cDescri
	ZCH->( msUnLock() )   
	
	Aviso("Aten็ใo", "Altera็ใo realizada!", {"OK"}, 03)
	
// Se for exclusao
ElseIf nOpc == MODEL_OPERATION_DELETE

	RecLock("ZCH", .F.)
		dbDelete()
	ZCH->( msUnLock() )   
	
	Aviso("Aten็ใo", "Exclusใo realizada!", {"OK"}, 03)
                              
EndIf

Return lRet


