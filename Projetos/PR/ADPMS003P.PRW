Static Function fAlterar( oModel )

Local lRet       := .t.
Local nOperation := oModel:GetOperation()

// Se for exclusao
If nOperation == MODEL_OPERATION_DELETE
	If RetCodUsr() <> "000000"
		lRet := .f.
		Aviso("Aten็ใo", "Somente o admin pode excluir registros!", {"OK"}, 03)
	EndIf
EndIf

Return lRet
